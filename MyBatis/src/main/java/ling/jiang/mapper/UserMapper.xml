<mapper namespace="ling.jiang.mapper.UserMapper">

    <select id="getUserById" resultType="user">
      select * from auth_user where user_id = #{id}
    </select>

    <select id="getAllUsers" resultType="ling.jiang.pojo.User">
      select * from auth_user
    </select>
</mapper>